        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-custom": "webpack",
        "start-custom": "webpack-dev-server --port=4200",
        "test-custom": "karma start ./karma.conf.js",
        "pree2e-custom": "webdriver-manager update --standalone false --gecko false --quiet",
