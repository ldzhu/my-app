        "i18n:create": "cd src/i18n && copy /y .\\messages.xlf .\\messages.zh-CN.xlf && copy /y .\\messages.xlf .\\messages.en-US.xlf",
        "i18n:kendo-translate": "./node_modules/.bin/kendo-translate src/i18n/messages.zh-CN.xlf --locale zh-CN"
    },
